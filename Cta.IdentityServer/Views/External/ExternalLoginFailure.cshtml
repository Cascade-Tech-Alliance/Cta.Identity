@model Cta.IdentityServer.Models.External.ExternalLoginFailureViewModel
@{
    ViewData["Title"] = "Unable to Sign In";
}

<br /><br /><br />
<hr style="width: 100%;" />
<h4>@ViewData["Title"].</h4>
<p class="text-info">Unable to sign in with those credentials.  The email address (@Model.Email) is not associated with an account in our system.</p>
@if (Model.RedirectUri != null)
{
    <div>
        Click <a class="PostLogoutRedirectUri" href="@Model.RedirectUri">here</a> to use different credentials.
    </div>
}